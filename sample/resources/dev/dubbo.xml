<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
	<dubbo:application name="${dubbo.mc.application}" />
	<dubbo:consumer check="false"></dubbo:consumer>  <!-- 关闭所有服务的启动时检查：(没有提供者时报错) -->
	<dubbo:registry address="${dubbo.registry.address}" />
	<dubbo:protocol name="dubbo" serialization="protobufmix"
		port="${dubbo.mc.port}" />
	<dubbo:service interface="com.zing.bubble.api.common.AdaptService"
		ref="adaptService"></dubbo:service>
	<dubbo:service interface="com.zing.bubble.api.inner.McService"
		ref="innerMcService"></dubbo:service>
	<dubbo:reference id="uniqueService"
		interface="com.zing.bubble.api.inner.UniqueService"></dubbo:reference>
</beans>
